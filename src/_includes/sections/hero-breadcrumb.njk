<section class="d-flex align-items-center page-hero  inner-page-hero " id="page-hero" style="height: 75vh; min-height: 75vh;">
    <div class="overlay-photo-image-bg parallax" data-bg-img="/assets/hero/inner-page-hero.jpg" data-bg-opacity="1"></div>
    <div class="overlay-color" data-bg-opacity=".75"></div>
    <div class="container">
        <div class="hero-text-area centerd">
            <h1 class="hero-title  wow fadeInUp" data-wow-delay=".2s">{{ title }}</h1>

            <nav aria-label="breadcrumb ">
                <ul class="breadcrumb wow fadeInUp" data-wow-delay=".6s">
                    <li class="breadcrumb-item">
                        <a class="breadcrumb-link" href="/">
                            <i class="bi bi-house icon "></i>home</a>
                    </li>
                    {% set breadcrumbPages = collections.all | eleventyNavigationBreadcrumb(title) %}
                    {% for breadcrumb in breadcrumbPages %}
                        <li class="breadcrumb-item">
                            <a href="{{breadcrumb.url}}" class="breadcrumb-link">{{breadcrumb.title}}</a>
                        </li>
                    {% endfor %}
                    <li class="breadcrumb-item active">{{ title }}</li>
                </ul>
            </nav>
        </div>
    </div>
</section>