<section class="d-flex align-items-center page-hero  inner-page-hero " id="page-hero" style="height: 75vh; min-height: 75vh;">
    <div class="overlay-photo-image-bg parallax" data-bg-img="/assets/images/hero/inner-page-hero.jpg" data-bg-opacity="1"></div>
    <div class="overlay-color" data-bg-opacity=".75"></div>
    <div class="container">
        <div class="hero-text-area centerd">
            <h1 class="hero-title  wow fadeInUp" data-wow-delay=".2s">Features</h1>
            <nav aria-label="breadcrumb ">
                <ul class="breadcrumb wow fadeInUp" data-wow-delay=".6s">
                    <li class="breadcrumb-item">
                        <a class="breadcrumb-link" href="/">
                            <i class="bi bi-house icon "></i>home</a>
                    </li>
                    <li class="breadcrumb-item active">features</li>
                </ul>
            </nav>

            {% set animationName = "features" %}
            {% set compositionId = "compositionFeatures" %}
            {% set sceneName = "sceneFeatures" %}
            {% set wrapperClass = "canvas-features" %}
            {% set width = 363 %}
            {% set height = 270 %}
            {% set position = "static" %}
            {% include "animations/animation-template.njk" %}
        </div>
    </div>
</section>